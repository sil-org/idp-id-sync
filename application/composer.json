{
  "name": "silinternational/idp-id-sync",
  "minimum-stability": "stable",
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-json": "*",
    "sil-org/idp-id-broker-php-client": "^3.0.0 || ^4.0",
    "sil-org/php-array-dot-notation": "^0.1.0",
    "sil-org/php-env": "^2.1.1",
    "sil-org/psr3-adapters": "^2.3 || ^3.0",
    "sil-org/yii2-json-log-targets": "^2.0",
    "yiisoft/yii2": "~2.0.15",
    "yiisoft/yii2-gii": "^2.0",
    "yiisoft/yii2-swiftmailer": "^2.0",
    "guzzlehttp/guzzle": "^6.2",
    "google/apiclient": "^2.0",
    "codemix/yii2-streamlog": "^1.3",
    "sentry/sentry": "^3.22",
    "sil-org/yii2-sentry": "^2.0"
  },
  "require-dev": {
    "behat/behat": "^3.3",
    "phpunit/phpunit": "^9.0",
    "friendsofphp/php-cs-fixer": "^2.9 || ^3.0",
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Sil\\Idp\\IdSync\\": "./",
      "Sil\\Idp\\IdSync\\Behat\\Context\\": "./features/bootstrap"
    }
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "php-http/discovery": false
    }
  }
}
